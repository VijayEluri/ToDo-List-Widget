		android:layout_alignParentTop="true" android:layout_alignParentRight="true"
		android:src="@drawable/background_99_0" android:paddingRight="7dp" android:paddingTop="7dp"
		android:paddingBottom="7dp" android:scaleType="fitEnd"/>
	
	<ImageView android:id="@+id/widget_scroll_down"
